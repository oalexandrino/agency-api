// About
section#about.page-section
	.container
		.row
			.col-lg-12.text-center
				h2.section-heading.text-uppercase About
				h3.section-subheading.text-muted Lorem ipsum dolor sit amet consectetur.
		.row
			.col-lg-12
				ul.timeline
					if homePageData[5].abouts
						each about in homePageData[5].abouts
							li
								.timeline-image
									img.rounded-circle.img-fluid(src='/images/agency/about/1.jpg', alt='')
								.timeline-panel
									.timeline-heading
										h4= about.date
										h4.subheading= about.headline
									.timeline-body
										p.text-muted
											= about.description
					li.timeline-inverted
						.timeline-image
							h4
								| Be Part
								br
								| Of Our
								br
								| Story!
